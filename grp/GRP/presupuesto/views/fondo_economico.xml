<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    
    <record id="presupuesto_fondo_economico_view_form" model="ir.ui.view">
      <field name="name">presupuesto.fondo_economico.form</field>
      <field name="model">presupuesto.fondo_economico</field>
      <field name="arch" type="xml">
        <form string="Listado de Fondo">
          <header/>
          <sheet string="Fondo">
            <group col="4" string="Fondo">
              <field name="fuente_financiamiento"/>
              <field name="denominacion"/>
            </group>
          </sheet>
        </form>
      </field>
    </record>

    <record id="presupuesto_fondo_economico_view_tree" model="ir.ui.view">
      <field name="name">presupuesto.fondo_economico.tree</field>
      <field name="model">presupuesto.fondo_economico</field>
      <field name="arch" type="xml">
        <tree>
          <field name="fuente_financiamiento"/>
          <field name="denominacion"/>
        </tree>
      </field>
    </record>

    <record id="presupuesto_fondo_economico_action" model="ir.actions.act_window">
      <field name="name">Fondo</field>
      <field name="res_model">presupuesto.fondo_economico</field>
      <field name="view_mode">tree,form</field>
      <field name="view_id" ref="presupuesto_fondo_economico_view_tree"/>
    </record>
  </data>
</odoo>
<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <!-- Comentario en la Views -->
    <record id="presupuesto_version_view_form" model="ir.ui.view">
      <field name="name">presupuesto.version.form</field>
      <field name="model">presupuesto.version</field>
      <field name="arch" type="xml">
        <form string="Listado de versión">
          <sheet string="Version">
            <group col="2" string="Versión">
              <field name="version"/>
              <field name="descripcion"/>
            </group>
          </sheet>
        </form>
      </field>
    </record>
    <record id="presupuesto_version_view_tree" model="ir.ui.view">
      <field name="name">presupuesto.version.tree</field>
      <field name="model">presupuesto.version</field>
      <field name="arch" type="xml">
        <tree>
          <field name="version"/>
          <field name="descripcion"/>
        </tree>
      </field>
    </record>
    <record id="presupuesto_version_action" model="ir.actions.act_window">
      <field name="name">Versión</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">presupuesto.version</field>
      <field name="view_mode">tree,form</field>
      <field name="view_id" ref="presupuesto_version_view_tree"/>
    </record>
  </data>
</odoo>
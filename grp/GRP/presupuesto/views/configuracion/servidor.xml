<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <!-- Comentario en la Views -->
    <record id="presupuesto_version_view_form" model="ir.ui.view">
      <field name="name">presupuesto.config_auxiliares.form</field>
      <field name="model">presupuesto.config_auxiliares</field>
      <field name="arch" type="xml">
        <form string="Listado de configuraciones">
          <sheet string="Version">
            <group col="4">
              <field name="status"/>
              <field name="server"/>
            </group>
          </sheet>
        </form>
      </field>
    </record>
    <record id="presupuesto_server_view_tree" model="ir.ui.view">
      <field name="name">presupuesto.config_auxiliares.tree</field>
      <field name="model">presupuesto.config_auxiliares</field>
      <field name="arch" type="xml">
        <tree>
          <field name="server"/>
          <field name="status"/>
        </tree>
      </field>
    </record>
    <record id="presupuesto_server_action" model="ir.actions.act_window">
      <field name="name">Configuracion de auxiliares</field>
      <field name="type">ir.actions.act_window</field>
      <field name="res_model">presupuesto.config_auxiliares</field>
      <field name="view_type">form</field>
      <field name="view_mode">tree,form</field>
    </record>
  </data>
</odoo>
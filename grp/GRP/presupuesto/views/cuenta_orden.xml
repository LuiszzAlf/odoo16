<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
  	<record id="presupuesto_cuenta_orden_view_tree" model="ir.ui.view">
      <field name="name">presupuesto.cuenta_orden.tree</field>
      <field name="model">presupuesto.cuenta_orden</field>
      <field name="arch" type="xml">
        <tree editable="bottom">
          <field name="posicion_presupuestaria"/>
          <field name="cta_orden_original_egreso"/>
          <field name="cta_orden_original_ingreso"/>
          <field name="cta_orden_modificado_egreso"/>
          <field name="cta_orden_modificado_ingreso"/>
          <field name="cta_orden_comprometido_egreso"/>
          <field name="cta_orden_comprometido_ingreso"/>
          <field name="cta_orden_devengado_egreso"/>
          <field name="cta_orden_devengado_ingreso"/>
          <field name="cta_orden_ejercido_egreso"/>
          <field name="cta_orden_ejercido_ingreso"/>
          <field name="cta_orden_pagado_egreso"/>
          <field name="cta_orden_pagado_ingreso"/>
        </tree>
      </field>
    </record>

  	<record id="presupuesto_cuenta_orden_action" model="ir.actions.act_window">
  	  <field name="name">Cuentas de orden</field>
  	  <field name="res_model">presupuesto.cuenta_orden</field>
  	  <field name="view_mode">tree</field>
      <field name="view_id" ref="presupuesto_cuenta_orden_view_tree"/>
  	</record>
  </data>
</odoo>